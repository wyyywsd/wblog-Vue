     @click=\"dd(article.ID)\"\n            target=\"_blank\"\n            class=\"article-title\"\n          >{{article.ArticleTitle}}</el-link>\n        </p>\n      </div>\n      <div>\n        <p class=\"article-content\">{{article.ArticleContent}}</p>\n      </div>\n      <div class=\"article-time\">\n        <p>作者头像 文章作者 {{article.CreatedAt | timeParse}} 热度</p>\n      </div>\n    </article>\n    <div>\n      <el-pagination background layout=\"prev, pager, next\" \t :total=\"1000\" style=\"margin-top:20px;\"></el-pagination>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapState } from \"vuex\";\nexport default {\n  data() {\n    return {\n      fullscreenLoading: true\n    };\n  },\n  computed: {\n    ...mapState(\"articles\", {\n      articles: \"articleList\",\n    }),\n  },\n  methods: {\n    ...mapActions(\"articles\", [\"loadData\"]),\n    dd(id) {\n      this.$router.push(\"/article/\" + id);\n    },\n  },\n\n  async mounted() {\n    let n = await this.loadData();\n    console.log(n)\n    \n    let loading = this.$loading({\n      lock: true,\n      text: \"Loading\",\n      spinner: \"el-icon-loading\",\n      background: \"rgba(255,255,255,1)\",\n    });\n    loading.close();\n  },\n  filters: {\n    timeParse() {\n      return \"格式化后的时间\";\n    },\n  },\n};\n</script>\n\n<style lang=\"stylus\" scoped>\n@import '~../../assets/stylus/ellipsis.styl'\n.article-title\n  font-size 20px\n  color #606266\n.article-content\n  ellipsis()\n  color #909399\n  font-size 16px\n.article-time\n  color #C0C4CC\n  font-size 13px\n  margin-bottom 50px\n  //border-bottom 1px solid #dcdfe6\n</style>"],"sourceRoot":"src/views/home"}]}