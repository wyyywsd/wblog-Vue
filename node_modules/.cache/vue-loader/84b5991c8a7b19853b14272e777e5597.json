>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapState } from \"vuex\";\nexport default {\n  data() {\n    return {};\n  },\n  computed: {\n    ...mapState(\"articles\", {\n      articles: \"articleList\",\n    }),\n  },\n  methods: {\n    ...mapActions(\"articles\", [\"loadData\"]),\n    dd(id) {\n      this.$router.push(\"/article/\" + id);\n    },\n  },\n  async mounted() {\n    let loading = this.$loading({\n      lock: true,\n      text: \"Loading\",\n      spinner: \"el-icon-loading\",\n      background: \"rgba(0, 0, 0, 0.7)\",\n    });\n\n    let n = await this.loadData();\n\n    loading.close();\n  },\n  filters: {\n    timeParse() {\n      return \"格式化后的时间\";\n    },\n  },\n};\n</script>\n\n<style lang=\"stylus\" scoped>\n@import '~../../assets/stylus/ellipsis.styl'\n.article-title\n  font-size 20px\n  color #606266\n.article-content\n  ellipsis()\n  color #909399\n  font-size 16px\n.article-time\n  color #C0C4CC\n  font-size 13px\n  border-bottom 1px solid #dcdfe6\n</style>"]}]}